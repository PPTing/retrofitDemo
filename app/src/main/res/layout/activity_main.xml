<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getDailyGank"/>
    <Button
        android:id="@+id/post"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="post retrofit mvp"/>
    <Button
        android:id="@+id/post_without_mvp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="post retrofit without mvp"/>
    <Button
        android:id="@+id/post_only_okhttp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="只使用 okhttp "/>
    <Button
        android:id="@+id/repo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="get_github_repo"/>
    <Button
        android:id="@+id/upload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="上传"/>
    <Button
        android:id="@+id/uploadMore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="多个文件"/>

</LinearLayout>
